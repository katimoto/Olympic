<div class='wrapper'>
  <div class='up1'>
    <%= form_with(url: search_questions_path, local: true, method: :get, class: "search-form", id: "qf") do |form| %>
      <%= form.text_field :keyword, placeholder: "タイトルで探す", class: "search-input", id: "qt" %>
      <%= form.submit "Go", class: "search-btn", id: "qs" %>
    <% end %>
    <%= render 'shared/up1'%>
  </div>
  <div class='down1'>
    <%= render 'shared/side_main' %>
    <div class='nav_main'>
      <div class='up2'>
        <%= link_to user_path(@user) do %>
          <p>ユーザートップ<p>
        <% end %>
      </div>
      <div class='down2'>
        <%= render "articles/header"%>
      </div>
    </div>
    <div class='main'>
      <div class='main_menu'>
        <h1 class="head_en">質問一覧</h1>
        <div class='aside'>
            <%= render 'users/heading'%>
        </div>
      </div>
      <div class='main_content'>
        <div class='question_wrapper'>
          <div class='question_index'>
            <p><%= @name %>さんの投稿一覧</p>
            <div class="contents low">
                <%= question_lists(@questions) %>
            </div>
            <%= paginate @questions %>
          </div>
        </div>
      </div>
      <%= link_to new_article_path, data: {"turbolinks"=>false} do %>
        <i class="fas fa-plus-circle firework"></i>
      <% end %>
    </div>
  </div>
  <div class='footer'>
  <p>@Copyright Olimpic Apps 2020</p>
  </div>
</div>