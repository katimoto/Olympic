

<div class='wrapper'>
  <div class='nav_main'>
    <%= render "header"%>
  </div>
  <div class='main'>
    <div class='main_menu'>
      <h1 class="head_en">My page</h1>
      <h3 class="head__ja">マイページ</h3>
      <%= render "heading"%>
    </div>
    <div class='main_content'>
      <div class="content">
        <div class='article_show'>
          <div class="contents row">
            <%= article_lists(@articles) %>
          </div>
          <%= paginate @articles %>
        </div>
      </div>
      <div class='aside'>
        <%= render "questions/aside"%>
      </div>
    </div>
  </div>
</div>



